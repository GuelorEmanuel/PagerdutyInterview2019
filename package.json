{
  "name": "PagerdutyInterview2019",
  "version": "1.0.0",
  "description": "You are on a quest as an elusive chef/programmer to sample the abundance of international cuisine from all over the world. In order to document your adventure and not risk forgetting the names of various delicious meals, you decide to code an application to help keep track of them",
  "main": "src/server/index.js",
  "scripts": {
    "go": "open http://localhost:3000; npm run server",
    "e2e": "nightwatch; npm run reset-data;",
    "reset-data": "cp data.json.pristine data.json",
    "test": "npm run reset-data; ./node_modules/.bin/concurrently -k 'npm run server' 'npm run e2e'",
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "private": true,
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "1.14.1",
    "express": "4.13.3",
    "fs": "0.0.2",
    "lodash": "^4.17.11",
    "path": "0.12.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "7.2.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "autoprefixer": "9.4.3",
    "babel-loader": "8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "2.1.0",
    "file-loader": "3.0.1",
    "gulp-babel": "^8.0.0",
    "html-webpack-plugin": "3.0.7",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.11.0",
    "nodemon": "^1.18.10",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "^4.6.5",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "webpack": "4.28.3",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.14"
  },
  "author": "Guelor Emanuel",
  "license": "MIT"
}
